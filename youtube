#!/bin/bash

help() {
   echo 'usage: youtube ["youtube query"|URL]'
   echo '       youtube --help'
   echo
   echo '   Access youtube via `google-wrapper`'
   echo
   echo '   Depends on `google-wrapper`'
   echo
   exit 1
}

[ "$1" == "--help" ] && help

google-wrapper "https://www.youtube.com/results?search_query=$@"

